<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%  @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
      <td> <%= user.last_name %></td>
      <td> <%= user.email %></td>
        <td><%= link_to 'Show', user, class: "btn btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
